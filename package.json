{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "react ssr demo, with react route webpack v4+",
  "main": "index.js",
  "scripts": {
    "webpack:client": "webpack --config config/webpack.client.config.js --color --progress --hide-modules --watch",
    "webpack:server": "webpack --config config/webpack.server.config.js --color --progress --hide-modules --watch",
    "webpack:start": "nodemon --watch dist --exec node dist/server.js",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel webpack:*",
    "start": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js && webpack --config config/webpack.server.config.js && node dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
